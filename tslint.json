{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "prettier": true,
        "trailing-comma": false
    },
    "rulesDirectory": ["tslint-plugin-prettier"],
    "linterOptions": {
        "exclude": ["**/node_modules/**"]
    }
}

